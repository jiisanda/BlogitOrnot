{
    "builds": [{
        "src": "BlogProject/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.11"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "BlogProject/wsgi.py"
        }
    ]
}